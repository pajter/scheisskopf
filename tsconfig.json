{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",

    "skipLibCheck": true,

    "strict": true,

    "noUnusedLocals": true,
    "noUnusedParameters": true,

    "noFallthroughCasesInSwitch": true,

    "moduleResolution": "node",
    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true /* Disallow inconsistently-cased references to the same file. */
  }
}
